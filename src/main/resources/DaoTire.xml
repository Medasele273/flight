<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id = "airlineDao" class = "se.lexicon.flight.daoImpl.AirlineDaoImpl">
        <constructor-arg name="airlineDao"  ref="airlineDao"/>
    </bean>

    <bean id="passengerDao" class="se.lexicon.flight.daoImpl.PassengerDaoImpl">
        <constructor-arg name ="passengerDao" ref="passengerDao"/>
    </bean>




</beans>